{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@libs/*": ["libs/*"],
      "@res/*": ["res/*"]
    },
    "target": "es5",
    "module": "commonjs",
    "lib": ["dom", "es5", "es6"],
    "resolveJsonModule": true
  },
  "rules": {
    "semicolon": true
  }
}
